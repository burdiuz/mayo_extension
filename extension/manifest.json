{
  "manifest_version": 2,
  "name": "MAYO",
  "version": "0.0.2",
  "description": "MAYO makes anything better",
  "icons":{
     "16": "icon16.png",
     "48": "icon48.png",
     "128": "icon128.png"
  },
  "browser_action":{
     "default_icon":{
        "16": "icon16.png",
        "24": "icon24.png",
        "32": "icon32.png"
     },
     "default_title": "Make it better with MAYO.",
     "default_popup": "popup.html"
  },
  "homepage_url": "https://github.com/burdiuz/mayo_extension",
  "offline_enabled": true,
  "permissions":[
    "activeTab"
  ],
  "short_name": "MAYO"
}
